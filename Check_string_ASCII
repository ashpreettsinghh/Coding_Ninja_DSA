#include <iostream>
int main() {
    char n;
    std::cout<<"enter anything";
    std::cin>>n;
    int y;
    y= (int) n;
    if(y>=65 && y<=122)
    {
        
    
        if(y>=65 && y<=90)
         std::cout<<"1";
        else if(y>=97 && y<=122)
            std::cout<<"0";
    }
    else
        std::cout<<"-1";
        
  return 0;
}
