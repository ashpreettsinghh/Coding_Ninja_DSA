#include <iostream> 
int main() {
    char n;
    std::cout<<"enter anything";
    std::cin>>n;
    
    if(isalpha(n)) 
    {
        
    
        if(isupper(n))
         std::cout<<"1";
        else if(islower(n))
            std::cout<<"0";
    }
    else
        std::cout<<"-1";
        
  return 0;
}
